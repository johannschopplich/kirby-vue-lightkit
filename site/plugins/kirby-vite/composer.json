{
  "name": "johannschopplich/kirby-vite",
  "description": "Vite integration for the Kirby CMS",
  "type": "kirby-plugin",
  "version": "1.0.3",
  "keywords": [
    "kirby",
    "vite"
  ],
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/kirby-vite#readme",
  "authors": [
    {
      "name": "Johann Schopplich",
      "email": "pkg@johannschopplich.com",
      "homepage": "https://johannschopplich.com"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "getkirby/composer-installer": "^1.2"
  },
  "require-dev": {
    "getkirby/cms": "^3.7"
  },
  "scripts": {
    "dist": "composer install --no-dev --optimize-autoloader"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "kirby-cms-path": false
  }
}
