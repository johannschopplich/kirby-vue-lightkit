{
  "private": true,
  "name": "kirby-vue-lightkit",
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public server.php\"",
    "dev": "shx touch src/.lock && concurrently \"npm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build",
    "lint": "eslint \"src/**/*.{ts,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,vue}\""
  },
  "dependencies": {
    "@vueuse/core": "^6.9.1",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.428",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "concurrently": "^6.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "2.4.1",
    "shx": "^0.3.3",
    "unplugin-icons": "^0.12.18",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vite-plugin-pages": "^0.18.2",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.1",
    "windicss": "^3.2.1"
  }
}
