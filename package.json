{
  "private": true,
  "name": "kirby-vue-lightkit",
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public server.php\"",
    "dev": "shx touch src/.lock && concurrently \"npm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "@vueuse/core": "^6.7.3",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.419",
    "@types/node": "^16.11.5",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "concurrently": "^6.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.20.0",
    "prettier": "2.4.1",
    "shx": "^0.3.3",
    "unplugin-icons": "^0.12.17",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.6.11",
    "vite-plugin-pages": "^0.18.1",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.4.12",
    "windicss": "^3.1.9"
  }
}
