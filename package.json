{
  "name": "kirby-vue-lightkit",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public server.php\"",
    "dev": "shx touch src/.lock && concurrently \"npm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build",
    "lint": "eslint \"src/**/*.{js,vue}\""
  },
  "dependencies": {
    "vue": "^3.0.11",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "concurrently": "^6.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^34.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^7.9.0",
    "sass": "^1.32.12",
    "shx": "^0.3.3",
    "vite": "^2.3.0",
    "vite-pages": "^0.1.2"
  }
}
