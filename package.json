{
  "private": true,
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public vendor/getkirby/cms/router.php\"",
    "dev": "shx touch src/.lock && concurrently \"npm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build",
    "lint": "eslint \"src/**/*.{ts,vue}\" --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{ts,vue}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.7",
    "@vueuse/core": "^9.1.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.93",
    "@types/node": "^18.7.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@unocss/preset-typography": "^0.45.7",
    "@unocss/preset-wind": "^0.45.7",
    "@unocss/transformer-directives": "^0.45.7",
    "@vitejs/plugin-vue": "^3.0.3",
    "concurrently": "^7.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "2.7.1",
    "shx": "^0.3.4",
    "unocss": "^0.45.7",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.7",
    "vite-plugin-pages": "^0.25.0"
  }
}
